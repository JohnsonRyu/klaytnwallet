{
  "name": "gx-dapp-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for react app with truffle",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "build:local": "NODE_ENV=local webpack --config webpack.prod.config.js",
    "build:dev": "NODE_ENV=dev webpack --config webpack.prod.config.js",
    "build:qa": "NODE_ENV=qa webpack --config webpack.prod.config.js",
    "build:real": "NODE_ENV=real webpack --config webpack.prod.config.js",
    "local": "NODE_ENV=local node ./klaytnwalletFront.js webpack.config.js --watch",
    "dev": "NODE_ENV=dev NEWRELIC_APP_NAME=KLAYTNWALLET_DEV_FRONT node --require newrelic ./klaytnwalletFront.js webpack.config.js --watch",
    "qa": "NODE_ENV=qa NEWRELIC_APP_NAME=KLAYTNWALLET_QA_FRONT node --require newrelic ./klaytnwalletFront.js webpack.prod.config.js --watch",
    "real": "NODE_ENV=real NEWRELIC_APP_NAME=KLAYTNWALLET_REAL_FRONT node --require newrelic ./klaytnwalletFront.js webpack.prod.config.js --watch",
    "start": "NODE_ENV=real NEWRELIC_APP_NAME=KLAYTNWALLET_REAL_FRONT node --require newrelic ./klaytnwalletFront.js webpack.prod.config.js --watch",
    "start:local": "NODE_ENV=local node ./klaytnwalletFront.js webpack.prod.config.js --watch"
  },
  "repository": {
    "type": "git"
  },
  "author": "nujabes403",
  "license": "GPL-3.0",
  "dependencies": {
    "bignumber.js": "^7.2.1",
    "caver-js": "0.0.2-v",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "dotenv": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "ethereumjs-tx": "^1.3.4",
    "ethereumjs-util": "^5.2.0",
    "ethereumjs-wallet": "^0.6.0",
    "js-cookie": "^2.2.0",
    "json-format": "^1.0.1",
    "lodash": "^4.17.10",
    "newrelic": "^4.8.1",
    "numeral": "^2.0.6",
    "onit-js": "0.0.1-z",
    "randombytes": "^2.0.6",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-dropdown": "^1.5.0",
    "react-lottie": "^1.2.3",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "react-tooltip": "^3.8.1",
    "redux": "^4.0.0",
